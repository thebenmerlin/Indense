{
    "expo": {
        "name": "Indense",
        "slug": "indense",
        "version": "1.0.0",
        "orientation": "default",
        "icon": "./assets/images/icon.png",
        "scheme": "indense",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "splash": {
            "image": "./assets/images/splash-icon.png",
            "resizeMode": "contain",
            "backgroundColor": "#1a365d"
        },
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.indense.app"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/adaptive-icon.png",
                "backgroundColor": "#1a365d"
            },
            "package": "com.indense.app"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": [
            "expo-router",
            "expo-secure-store"
        ],
        "experiments": {
            "typedRoutes": true
        }
    }
}