# Backend Environment Variables

# Database
DATABASE_URL="postgresql://user:password@localhost:5432/material_management?schema=public"

# JWT
JWT_SECRET="your-super-secret-jwt-key-change-in-production"
JWT_EXPIRES_IN="15m"
JWT_REFRESH_EXPIRES_IN="7d"

# Server
PORT=3000
NODE_ENV=development

# File Storage
UPLOAD_DIR="./uploads"
MAX_FILE_SIZE=10485760

# Logging
LOG_LEVEL=debug
